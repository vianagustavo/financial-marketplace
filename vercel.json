{
    "version": 2,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "out"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "out/$1"
        }
    ],
    "functions": {
        "api/index.js": {
            "runtime": "vercel-dotnet-8.0"
        }
    }
}